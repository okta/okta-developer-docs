								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "SSWS {{apikey}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"name\": \"cornerstone\",\n  \"label\": \"created via API\",\n  \"signOnMode\": \"SAML_2_0\",\n  \"settings\": {\n    \"app\": {\n      \"orgName\": \"dan\",\n      \"acsURL\": null,\n      \"url\": \"https://example.com/login.html\"\n    },\n     \"signOn\": {\n     \t    \"destinationOverride\": \"http://www.okta.com\",\n            \"attributeStatements\": [\n                    {\n                        \"type\": \"EXPRESSION\",\n                        \"name\": \"nameFromAPI\",\n                        \"namespace\": \"urn:oasis:names:tc:SAML:2.0:attrname-format:unspecified\",\n                        \"values\": [\n                            \"Value1FromAPI\",\n                            \"Value2FromAPI\"\n                        ]\n                    },\n                        {\n                        \"type\": \"EXPRESSION\",\n                        \"name\": \"name2FromAPI\",\n                        \"namespace\": \"urn:oasis:names:tc:SAML:2.0:attrname-format:unspecified\",\n                        \"values\": [\n                            \"Value1FromAPI\",\n                            \"Value2FromAPI\",\n                            \"Value3FromAPI\"\n                        ]\n                    },\n                    {\n                        \"type\": \"GROUP\",\n                        \"name\": \"GroupNameFromAPI\",\n                        \"namespace\": \"urn:oasis:names:tc:SAML:2.0:attrname-format:unspecified\",\n                        \"filterType\": \"STARTS_WITH\",\n                        \"filterValue\": \"ValueFromAPI\"\n                    }\n                ]\n    }\n  }\n}"
						},
						"url": {
							"raw": "{{url}}/api/v1/apps",
							"host": [
								"{{url}}"
							],
							"path": [
								"api",
								"v1",
								"apps"
							]
						},
						"description": "Add an instance of a app with SAML 2.0 signon mode"
					},
					"response": []
				},
